{
  "app": "vendor",
  "version": "1.0.0",
  "baseUrl": "https://your-domain.com",
  "description": "Vendor mobile app API documentation for BuyLock marketplace - comprehensive vendor dashboard for managing products, services, orders, and earnings",
  "authentication": {
    "type": "vendor_auth",
    "description": "Uses custom vendor authentication with vendor ID header",
    "headers": {
      "required": {
        "x-vendor-id": "vendor_id_here",
        "Content-Type": "application/json"
      }
    },
    "note": "All API calls require x-vendor-id header for vendor identification"
  },
  "pages": [
    {
      "name": "Vendor Login",
      "route": "/login",
      "description": "Vendor authentication page",
      "authentication": "none",
      "apiCalls": [
        {
          "purpose": "Vendor login",
          "method": "POST",
          "url": "/api/vendor/login",
          "auth": "none",
          "headers": {
            "Content-Type": "application/json"
          },
          "requestBody": {
            "email": "vendor@example.com",
            "password": "password"
          },
          "response": {
            "type": "VendorAuth",
            "example": {
              "success": true,
              "vendor": {
                "id": "vendor_id",
                "businessName": "My Business",
                "email": "vendor@example.com",
                "verified": true
              },
              "token": "vendor_auth_token"
            }
          },
          "errors": [
            {
              "status": 401,
              "message": "Invalid credentials"
            },
            {
              "status": 403,
              "message": "Account not verified"
            }
          ]
        }
      ]
    },
    {
      "name": "Vendor Dashboard",
      "route": "/dashboard",
      "description": "Main dashboard with vendor statistics and overview",
      "authentication": "required",
      "apiCalls": [
        {
          "purpose": "Get vendor information",
          "method": "GET",
          "url": "/api/auth/vendor",
          "auth": "required",
          "headers": {
            "x-vendor-id": "vendor_id"
          },
          "response": {
            "type": "Vendor",
            "example": {
              "id": "vendor_id",
              "businessName": "My Business",
              "contactName": "John Doe",
              "email": "vendor@example.com",
              "phone": "+254700000000",
              "address": "Business address",
              "verified": true,
              "description": "Business description"
            }
          }
        },
        {
          "purpose": "Get vendor products",
          "method": "GET",
          "url": "/api/products",
          "auth": "required",
          "headers": {
            "x-vendor-id": "vendor_id"
          },
          "response": {
            "type": "Product[]"
          }
        },
        {
          "purpose": "Get vendor services",
          "method": "GET",
          "url": "/api/services",
          "auth": "required",
          "headers": {
            "x-vendor-id": "vendor_id"
          },
          "response": {
            "type": "Service[]"
          }
        },
        {
          "purpose": "Get dashboard statistics",
          "method": "GET",
          "url": "/api/vendor/stats",
          "auth": "required",
          "headers": {
            "x-vendor-id": "vendor_id"
          },
          "response": {
            "type": "VendorStats",
            "example": {
              "totalProducts": 25,
              "totalServices": 5,
              "activeListings": 28,
              "totalOrders": 150,
              "totalRevenue": "250000.00",
              "pendingOrders": 3
            }
          }
        }
      ]
    },
    {
      "name": "Products Management",
      "route": "/products",
      "description": "Manage vendor products - create, edit, delete",
      "authentication": "required",
      "apiCalls": [
        {
          "purpose": "Get vendor products",
          "method": "GET",
          "url": "/api/vendor/:vendorId/products",
          "auth": "required",
          "pathParams": {
            "vendorId": "vendor_id"
          },
          "headers": {
            "x-vendor-id": "vendor_id"
          },
          "response": {
            "type": "Product[]",
            "example": [
              {
                "id": "uuid",
                "name": "Product Name",
                "price": "1000.00",
                "stock": 50,
                "active": true,
                "adminApproved": true,
                "rating": "4.5",
                "reviewCount": 25
              }
            ]
          }
        },
        {
          "purpose": "Create new product",
          "method": "POST",
          "url": "/api/vendor/:vendorId/products",
          "auth": "required",
          "pathParams": {
            "vendorId": "vendor_id"
          },
          "headers": {
            "x-vendor-id": "vendor_id",
            "Content-Type": "application/json"
          },
          "requestBody": {
            "name": "Product Name",
            "description": "Product description",
            "price": 1000.00,
            "categoryId": "category_uuid",
            "subcategoryId": "subcategory_uuid",
            "brandId": "brand_uuid",
            "stock": 50,
            "imageUrl": "product_image_url",
            "tags": ["tag1", "tag2"]
          },
          "response": {
            "type": "Product"
          }
        },
        {
          "purpose": "Update product",
          "method": "PATCH",
          "url": "/api/vendor/:vendorId/products/:productId",
          "auth": "required",
          "pathParams": {
            "vendorId": "vendor_id",
            "productId": "product_id"
          },
          "headers": {
            "x-vendor-id": "vendor_id",
            "Content-Type": "application/json"
          },
          "requestBody": {
            "name": "Updated Product Name",
            "price": 1200.00,
            "stock": 75,
            "active": true
          }
        },
        {
          "purpose": "Delete product",
          "method": "DELETE",
          "url": "/api/vendor/:vendorId/products/:productId",
          "auth": "required",
          "pathParams": {
            "vendorId": "vendor_id",
            "productId": "product_id"
          },
          "headers": {
            "x-vendor-id": "vendor_id"
          }
        },
        {
          "purpose": "Get categories for product creation",
          "method": "GET",
          "url": "/api/categories",
          "auth": "none",
          "response": {
            "type": "Category[]"
          }
        },
        {
          "purpose": "Get subcategories for category",
          "method": "GET",
          "url": "/api/subcategories",
          "auth": "none",
          "queryParams": {
            "categoryId": "category_id"
          },
          "response": {
            "type": "Subcategory[]"
          }
        },
        {
          "purpose": "Get brands",
          "method": "GET",
          "url": "/api/brands",
          "auth": "none",
          "response": {
            "type": "Brand[]"
          }
        }
      ]
    },
