            "vendorId": "vendor_id"
          },
          "headers": {
            "x-vendor-id": "vendor_id"
          },
          "queryParams": {
            "period": "month|quarter|year",
            "startDate": "2024-01-01",
            "endDate": "2024-12-31"
          },
          "response": {
            "type": "EarningsSummary",
            "example": {
              "totalEarnings": "250000.00",
              "availableBalance": "200000.00",
              "pendingBalance": "50000.00",
              "totalWithdrawn": "100000.00",
              "commissionRate": "20",
              "totalCommissionPaid": "50000.00"
            }
          }
