    {
      "name": "Orders Management",
      "route": "/orders",
      "description": "Manage vendor orders - view, accept, update status",
      "authentication": "required",
      "apiCalls": [
        {
          "purpose": "Get vendor orders",
          "method": "GET",
          "url": "/api/vendor/:vendorId/orders",
          "auth": "required",
          "pathParams": {
            "vendorId": "vendor_id"
          },
          "headers": {
            "x-vendor-id": "vendor_id"
          },
          "queryParams": {
            "status": "pending|confirmed|shipped|delivered|cancelled",
            "limit": "50"
          },
          "response": {
            "type": "Order[]",
            "example": [
              {
                "id": "uuid",
                "status": "pending",
                "totalAmount": "5000.00",
                "customerName": "Customer Name",
                "deliveryAddress": "Customer address",
                "createdAt": "2024-01-01T00:00:00Z",
                "orderItems": [
                  {
                    "name": "Product Name",
                    "quantity": 2,
                    "price": "1000.00"
                  }
                ]
              }
            ]
          }
        },
        {
          "purpose": "Get order details",
          "method": "GET",
          "url": "/api/vendor/:vendorId/orders/:orderId",
          "auth": "required",
          "pathParams": {
            "vendorId": "vendor_id",
            "orderId": "order_id"
          },
          "headers": {
            "x-vendor-id": "vendor_id"
          },
          "response": {
            "type": "OrderWithDetails"
          }
        },
        {
          "purpose": "Accept order",
          "method": "PATCH",
          "url": "/api/vendor/:vendorId/orders/:orderId/accept",
          "auth": "required",
          "pathParams": {
            "vendorId": "vendor_id",
            "orderId": "order_id"
          },
          "headers": {
            "x-vendor-id": "vendor_id",
            "Content-Type": "application/json"
          },
          "requestBody": {
            "estimatedPreparationTime": "2 hours",
            "vendorNotes": "Order confirmed and in preparation"
          }
        },
        {
          "purpose": "Update order status",
          "method": "PATCH",
          "url": "/api/vendor/:vendorId/orders/:orderId/status",
          "auth": "required",
          "pathParams": {
            "vendorId": "vendor_id",
            "orderId": "order_id"
          },
          "headers": {
            "x-vendor-id": "vendor_id",
            "Content-Type": "application/json"
          },
          "requestBody": {
            "status": "ready_for_pickup",
            "notes": "Order is ready for courier pickup"
          }
        },
        {
          "purpose": "Mark order as ready for pickup",
          "method": "PATCH",
          "url": "/api/vendor/:vendorId/orders/:orderId/ready",
          "auth": "required",
          "pathParams": {
            "vendorId": "vendor_id",
            "orderId": "order_id"
          },
          "headers": {
            "x-vendor-id": "vendor_id"
          }
        },
        {
          "purpose": "Cancel order",
          "method": "PATCH",
          "url": "/api/vendor/:vendorId/orders/:orderId/cancel",
          "auth": "required",
          "pathParams": {
            "vendorId": "vendor_id",
            "orderId": "order_id"
          },
          "headers": {
            "x-vendor-id": "vendor_id",
            "Content-Type": "application/json"
          },
          "requestBody": {
            "reason": "Out of stock"
          }
        }
      ]
    },
    {
      "name": "Earnings Management",
      "route": "/earnings",
      "description": "Track earnings, payouts, and financial analytics",
      "authentication": "required",
      "apiCalls": [
        {
          "purpose": "Get vendor earnings summary",
